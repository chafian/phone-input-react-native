{
  "name": "@chafian/phone-input-react-native",
  "version": "0.1.0",
  "description": "Advanced, customizable phone input component for React Native and Expo with multi-language support, RTL, validation, and modern SVG flags",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/chafian/phone-input-react-native.git"
  },
  "homepage": "https://github.com/chafian/phone-input-react-native#readme",
  "bugs": {
    "url": "https://github.com/chafian/phone-input-react-native/issues"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate-flags": "tsx scripts/generateFlags.ts"
  },
  "keywords": [
    "react-native",
    "expo",
    "phone-input",
    "phone-number",
    "international",
    "libphonenumber",
    "validation",
    "formatting",
    "rtl",
    "i18n",
    "flags",
    "country-picker"
